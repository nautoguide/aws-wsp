    "shelljs": "^0.8.4",
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.1",
    "@babel/preset-env": "^7.13.5"
  },
  "dependencies": {
    "uuid": "^8.3.2",
    "js-base64": "^3.6.0"
  },
  "publishConfig": {
    "access": "public"
